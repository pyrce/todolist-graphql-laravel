{
    "devDependencies": {
        "@babel/plugin-proposal-decorators": "^7.12.1",
        "@babel/preset-react": "7.0.0",
        "axios": "^0.21.0",
        "core-js": "^3.0.0",
        "cross-env": "^7.0.2",
        "laravel-mix": "^5.0.1",
        "prop-types": "^15.7.2",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
